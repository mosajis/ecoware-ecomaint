{
  "name": "ecomaint-backend",
  "version": "1.0.50",
  "scripts": {
    "prisma:generate": "prisma generate --schema=./orm/schema.prisma && bun run fix:prisma-id",
    "prisma:pull": "prisma db pull --schema=./orm/schema.prisma --force && prisma-case-format --file ./orm/schema.prisma -p && bun run prisma:generate && bun run prisma:push",
    "prisma:push": "prisma db push --schema=./orm/schema.prisma && bun run prisma:generate",
    "prisma:format": "prisma format --schema=./orm/schema.prisma",
    "prisma:studio": "prisma studio --schema=./orm/schema.prisma",
    "fix:prisma-id": "node scripts/fix-prisma-id.js",
    "fix:prisma-sequence": "node scripts/fix-prisma-sequence.js",
    "generate:api": "bun scripts/generate-api.js",
    "dev": "bun run --watch src/main.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.19.0",
    "@prisma/sdk": "^4.0.0",
    "@sinclair/typebox": "^0.34.41",
    "bcrypt": "^6.0.0",
    "elysia": "latest",
    "mssql": "^11.0.1",
    "prismabox": "^1.1.25",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/mssql": "^9.1.8",
    "bun-types": "latest",
    "prisma": "^6.16.2",
    "prisma-case-format": "^2.2.1",
    "ts-morph": "^27.0.2"
  },
  "module": "src/index.js"
}
