{
  "name": "backend",
  "version": "1.0.50",
  "scripts": {
    "db:prisma": "prisma --schema=./orm/schema.prisma",
    "db:pull": "prisma db pull --schema=./orm/schema.prisma --force && prisma-case-format --file ./orm/schema.prisma -p && prisma generate --schema=./orm/schema.prisma  && bun run db:push",
    "db:push": "prisma db push --schema=./orm/schema.prisma && prisma generate --schema=./orm/schema.prisma",
    "db:format": "prisma format --schema=./orm/schema.prisma",
    "db:generate": "prisma generate --schema=./orm/schema.prisma",
    "db:studio": "prisma studio --schema=./orm/schema.prisma",
    "schema:update": "node scripts/update-schema.js",
    "generate:api": "bun scripts/generate-api.js",
    "dev": "bun run --watch src/main.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.16.2",
    "@sinclair/typebox": "^0.34.41",
    "bcrypt": "^6.0.0",
    "elysia": "latest",
    "mssql": "^11.0.1",
    "prismabox": "^1.1.25"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/mssql": "^9.1.8",
    "bun-types": "latest",
    "prisma": "^6.16.2",
    "prisma-case-format": "^2.2.1"
  },
  "module": "src/index.js"
}
