{
  "name": "ecomaint-backend",
  "version": "1.0.50",
  "scripts": {
    "prisma:generate": "prisma generate --schema=./orm/schema.prisma && bun run prisma:fix-client-ids",
    "prisma:pull": "prisma db pull --schema=./orm/schema.prisma --force && bun run prisma:fix-schema-timestamps && bun run prisma:fix-schema-sequences && prisma-case-format --file ./orm/schema.prisma -p && bun run prisma:format && bun run prisma:generate && bun run prisma:push",
    "prisma:push": "prisma db push --schema=./orm/schema.prisma && bun run prisma:generate",
    "prisma:format": "prisma format --schema=./orm/schema.prisma",
    "prisma:studio": "prisma studio --schema=./orm/schema.prisma",
    "prisma:fix-schema-timestamps": "bun run ./scripts/fix-schema-timestamps.js",
    "prisma:fix-schema-sequences": "bun run ./scripts/fix-schema-sequences.js",
    "prisma:fix-client-ids": "bun run ./scripts/fix-client-ids.js",
    "generate:api": "bun scripts/generate-api.js",
    "dev": "bun run --watch src/main.ts",
    "build": "bun run ./scripts/build.js"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.13",
    "@elysiajs/static": "^1.4.7",
    "@sinclair/typebox": "^0.34.41",
    "bcrypt": "^6.0.0",
    "elysia": "latest",
    "lodash": "^4.17.21",
    "mssql": "^12.2.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@types/bcrypt": "^6.0.0",
    "@types/mssql": "^9.1.8",
    "bun-types": "latest",
    "prisma": "^6.16.2",
    "prismabox": "^1.1.26",
    "prisma-case-format": "^2.2.1",
    "ts-morph": "^27.0.2"
  },
  "module": "src/mian.ts"
}
